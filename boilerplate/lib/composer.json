{
    "name": "zestphp/web-app",
    "description": "A ZestPHP Web application",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "twig/twig": "^3.0",
        "twig/extra-bundle": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "../classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
